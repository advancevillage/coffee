<div class="list_wrapper">
    <div class="list_activity_area">
        <div id="list_activity_area_workspace">

        </div>
        <div id="list_activity_area_countdown">

        </div>
    </div>
    <% if (locals.params.navigate['main']) { %>
        <% for(var i = 0; i < params.navigate['main'].length; i++) { %>
            <% var item = params.navigate['main'][i] %>
            <div class="list_item" data-key="<%= item.key %>" data-catid="<%= item.cat %>">
                <!--IndexPage 商品分类轮播-->
                <div class="list_item_banner">
                    <div class="swiper-container list_item_banner_swiper">
                        <div class="swiper-wrapper">
                            <div class="swiper-slide list_item_banner_slide">Slide 1</div>
                            <div class="swiper-slide list_item_banner_slide">Slide 2</div>
                            <div class="swiper-slide list_item_banner_slide">Slide 3</div>
                            <div class="swiper-slide list_item_banner_slide">Slide 4</div>
                        </div>
                        <div class="swiper-pagination  list_item_banner_pagination"></div>
                        <div class="swiper-button-next list_item_banner_next"></div>
                        <div class="swiper-button-prev list_item_banner_prev"></div>
                    </div>
                </div>
                <!--IndexPage 商品展示区-->
                <div class="list_item_goods">
                    <% if (locals.params.goods[item.key]) { %>
                        <% for(var j = 0; j < params.goods[item.key].length; j++) { %>
                            <% var sku = params.goods[item.key][j] %>
                            <div class="list_item_goods_sku" data-sku-key="<%= sku.sku_key%>"></div>
                        <% } %>
                    <% } %>
                </div>
            </div>
        <% } %>
    <% } %>
</div>
